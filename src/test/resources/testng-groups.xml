<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="Alatron Group Suite">

    <test name="Smoke Tests">
        <groups>
            <run>
                <include name="smoke"/>
            </run>
        </groups>

        <classes>
            <class name="tests.auth.VendorLoginTest"/>
        </classes>
    </test>

    <test name="Regression Tests">
        <groups>
            <run>
                <include name="regression"/>
            </run>
        </groups>

        <classes>
            <class name="tests.auth.VendorLoginTest"/>
        </classes>
    </test>

</suite>
